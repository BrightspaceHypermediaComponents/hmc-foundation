{
	"extends": "brightspace/polymer-3-config",
	"env": {
    "es6": true,
    "jest": true
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": {
      "sourceType": "module",
      "requireConfigFile": false
  },
  "rules": {
    "no-var": "error",
    "prefer-const": "error"
  },
  "globals": {
    "fastdom": false
  }
}
